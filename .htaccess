<FilesMatch "(diff-)?plan-[0-9]air1(_old)?\.(png|jpg|jpeg)">
    <IfModule mod_headers.c>
        Header unset ETag
	    Header set Cache-Control "no-cache, no-store, must-revalidate"
	    Header set Pragma "no-cache"
	    Header set Expires 0
		Header set Pragma-directive "no-cache"
		Header set Cache-directive "no-cache"
    </IfModule>
</FilesMatch>
